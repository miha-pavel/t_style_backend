{
    "files.autoSave": "onFocusChange",
    "files.watcherExclude": {
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "editor.wordWrapColumn": 120,
    "editor.bracketPairColorization.enabled": true,
    "editor.suggest.preview": true,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "all",
    "editor.formatOnSave": true,
    "editor.mouseWheelZoom": true,
    "editor.accessibilitySupport": "on",
    "editor.suggestSelection": "first",
    "vscode-postgres.defaultConnection": "127.0.0.1",
    "vscode-postgres.prettyPrintJSONfields": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.views.contributors.files.compact": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": true,
        "suppressCommitNotFoundWarning": true,
        "suppressCreatePullRequestPrompt": true
    },
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "diffEditor.wordWrap": "on",
    "diffEditor.maxComputationTime": 0,
    "window.title": "${dirty}${activeEditorLong}",
    "window.zoomLevel": 1,
    "cSpell.userWords": [
        "aiomysql",
        "apostrophed",
        "AREPL",
        "asdict",
        "ASGI",
        "asyncio",
        "Authorised",
        "binom",
        "boto",
        "botocore",
        "bson",
        "chmod",
        "cond",
        "confluentinc",
        "datetime",
        "dateutil",
        "dbname",
        "dont",
        "dotenv",
        "dynload",
        "elif",
        "getenv",
        "Gmail",
        "groupby",
        "gunicorn",
        "HEALTHCHECK",
        "hseeberger",
        "httpx",
        "ipynb",
        "isalnum",
        "isalpha",
        "isdigit",
        "islower",
        "issubset",
        "isupper",
        "iterrows",
        "lenght",
        "logfile",
        "mongodb",
        "Monokai",
        "msal",
        "noqa",
        "objectdb",
        "openapi",
        "oraclelinux",
        "Pavlo",
        "pbkdf",
        "POSTGRES",
        "prefs",
        "psycopg",
        "pycountry",
        "pygame",
        "pylint",
        "pymongo",
        "pytest",
        "PYTHONUNBUFFERED",
        "pytz",
        "rmtree",
        "rsplit",
        "scenarious",
        "socketio",
        "starlette",
        "timeframe",
        "timescaledb",
        "undos",
        "unstub",
        "valign",
        "VCSs",
        "vsicons",
        "vsintellicode",
        "werkzeug",
        "WORKDIR",
        "xargs",
        "XSRF",
        "yoyo",
        "zfill",
        "zxcvbn",
        "—Åorrect"
    ],
    "yaml.format.printWidth": 120,
    "yaml.format.singleQuote": true,
    "yaml.trace.server": "messages",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vsintellicode.java.completionsEnabled": false,
    "vsintellicode.typescript.completionsEnabled": false,
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.extensionsPath": [
        ""
    ],
    "settingsSync.ignoredExtensions": [],
    "settingsSync.ignoredSettings": [],
    "workbench.editor.decorations.colors": true,
    "workbench.editor.decorations.badges": true,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Monokai",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "notebook.outline.showCodeCells": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "AREPL.unsafeKeywords": [
        "rmdir\\(",
        "rmtree\\("
    ],
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "prettier.jsxSingleQuote": true,
    "prettier.printWidth": 120,
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "python.testing.debugPort": 5000,
    "python.testing.unittestEnabled": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        ".",
        "-p",
        "test_*.py"
    ],
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.rulers": [
            {
                "column": 80,
                "color": "#00ff00"
            },
            {
                "column": 120,
                "color": "#ff0000"
            }
        ]
    },
    "pythonIndent.trimLinesWithOnlyWhitespace": true,
    "debug.inlineValues": "on",
    "debug.autoExpandLazyVariables": true,
    "telemetry.telemetryLevel": "off",
    "redhat.telemetry.enabled": false,
    "AREPL.defaultFilterTypes": [
        "<class 'module'>",
        "<class 'function'>"
    ],
    "AREPL.telemetry": false,
    "vsicons.dontShowNewVersionMessage": true,
    "terminal.integrated.localEchoExcludePrograms": [
        "vim"
    ],
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.profiles.linux": {
        "bash": null,
        "JavaScript Debug Terminal": null,
        "zsh": null,
        "bash (2)": {
            "path": "/usr/bin/bash",
            "icon": "terminal-bash"
        }
    },
    "terminal.integrated.defaultProfile.linux": "bash (2)",
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "flake8.showNotifications": "always",
    "projectManager.git.baseFolders": [
        "~/Pavlo/work/t_style_backend"
    ],
    "terminal.integrated.customGlyphs": false,
    "pylint.args": [
        "\"pylint.args\": [\"--rcfile=.pylintrc\"]"
    ],
    "pylint.ignorePatterns": [
        "venv/, tmp.py, .nox/, "
    ]
}
